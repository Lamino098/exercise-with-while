#import
import random
import time

#guess programm random number generator
num = random.randint(1, 1000000)
print(num)
print()
time.sleep(5)

#time
start_time = time.time()

#guess programm
 
guess = str(print("Indovina un numero compreso tra 1 e 1000: "))

while guess != num:
    num_random = random.randint(1, 1000000)
    time.sleep(0)
    
    guess = str(print(num_random))
    
    if num_random  == num:
        print("Indovinato!, il numero corretto Ã¨ " + str(num_random))
        break
    

end_time = time.time()

elapsed_time = end_time - start_time
print(elapsed_time)

  